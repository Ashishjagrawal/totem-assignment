{
  "name": "totem-memory-warehouse",
  "version": "1.0.0",
  "description": "Long-Term Memory Warehouse for Autonomous Agents",
  "main": "src/server.js",
  "scripts": {
    "start": "node src/server.js",
    "dev": "nodemon src/server.js",
    "test": "jest --coverage",
    "test:unit": "jest src/tests/unit --coverage",
    "test:integration": "jest src/tests/integration --coverage",
    "test:watch": "jest --watch",
    "test:all": "node src/tests/run-tests.js",
    "test:report": "node src/tests/generate-test-report.js",
    "db:migrate": "npx prisma migrate dev",
    "db:generate": "npx prisma generate",
    "db:seed": "node src/scripts/seed.js",
    "db:reset": "npx prisma migrate reset",
    "build": "npm run db:generate",
    "postinstall": "npm run db:generate",
    "lint": "eslint src/",
    "lint:fix": "eslint src/ --fix",
    "format": "prettier --write src/",
    "docker:build": "docker build -t memory-warehouse .",
    "docker:run": "docker-compose up -d",
    "docker:stop": "docker-compose down",
    "docker:logs": "docker-compose logs -f"
  },
  "keywords": [
    "memory",
    "ai",
    "agents",
    "postgresql",
    "vector",
    "embeddings"
  ],
  "author": "Ashish Jagrawal",
  "license": "MIT",
  "dependencies": {
    "express": "^4.21.2",
    "cors": "^2.8.5",
    "helmet": "^8.0.0",
    "morgan": "^1.10.0",
    "dotenv": "^16.4.7",
    "prisma": "^5.22.0",
    "@prisma/client": "^5.22.0",
    "pg": "^8.13.1",
    "pgvector": "^0.1.8",
    "openai": "^4.67.3",
    "axios": "^1.7.9",
    "winston": "^3.17.0",
    "node-cron": "^3.0.3",
    "compression": "^1.7.4",
    "express-rate-limit": "^7.4.1",
    "joi": "^17.13.3",
    "uuid": "^11.0.3"
  },
  "devDependencies": {
    "nodemon": "^3.1.9",
    "jest": "^29.7.0",
    "supertest": "^7.0.0",
    "eslint": "^9.17.0",
    "prettier": "^3.4.2"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
